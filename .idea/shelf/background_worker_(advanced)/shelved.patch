<+>import { Context, initialSession } from '~/context'\nimport { Router } from '@grammyjs/router'\n\nexport enum step {\n  'target:no_today:no',\n  'target:yes_today:no',\n  'target:yes_today:yes',\n}\n\nconst provideTarget =\n  'Please provide target wake-up time as `15:05` or simply as `1505`'\nconst provideToday =\n  'Please provide today wake-up time as `15:05` or simply as `1505`'\n\nexport const router = new Router<Context>((ctx: Context) =>\n  ctx.session.step.toString()\n)\n\nrouter.route(step['target:no_today:no'].toString(), async (ctx: Context) => {\n  console.info('step', step['target:no_today:no'].toString())\n  const target = parseTimestamp(ctx.msg?.text ?? '')\n  if (!target) {\n    await ctx.reply(provideTarget)\n    return\n  }\n\n  ctx.session.target = target\n  ctx.session.today = null\n  ctx.session.step = step['target:yes_today:no']\n  await ctx.reply(\n    `Successfully saved target time: ${extractTimestamp(\n      target\n    )}. Use command /today`\n  )\n})\n\nrouter.route(step['target:yes_today:no'].toString(), async (ctx: Context) => {\n  console.info('step', step['target:yes_today:no'].toString())\n  const today = parseTimestamp(ctx.msg?.text ?? '')\n  if (!today) {\n    await ctx.reply(provideToday)\n    return\n  }\n\n  ctx.session.today = today\n  ctx.session.step = step['target:yes_today:yes']\n  await updateDays(ctx)\n})\n\nrouter.route(step['target:yes_today:yes'].toString(), async (ctx: Context) => {\n  console.info('step', step['target:yes_today:yes'].toString())\n  await ctx.reply(\n    `Sorry, I didn't recognise this command. Your current settings: ${settings(\n      ctx\n    )} Use command /progress`\n  )\n})\n\nexport async function handleHelp(ctx: Context) {\n  console.info('command:help')\n  await ctx.reply(ctx.i18n.t('help'), { parse_mode: 'MarkdownV2' })\n}\n\nexport async function handleStart(ctx: Context) {\n  console.info('command:start')\n  ctx.session = initialSession()\n  const smile = ctx.emoji`${'smiling_face_with_sunglasses'}`\n  await ctx.reply(\n    `Welcome ${smile}! Starting (or restarting) the bot: ${settings(ctx)}`\n  )\n}\n\nexport async function handleSettings(ctx: Context) {\n  console.info('command:settings')\n  await ctx.reply(settings(ctx))\n}\n\nexport async function handleProgress(ctx: Context) {\n  console.info('command:progress')\n  if (ctx.session.step == step['target:no_today:no']) {\n    await ctx.reply(provideTarget)\n    return\n  }\n  await ctx.reply(`You are doing great for ${ctx.session.days} days`)\n}\n\nexport async function handleTarget(ctx: Context) {\n  console.info('command:target')\n  ctx.session.step = step['target:no_today:no']\n  await ctx.reply(provideTarget)\n}\n\nexport async function handleToday(ctx: Context) {\n  console.info('command:today')\n  switch (ctx.session.step) {\n    case step['target:no_today:no']:\n      await ctx.reply(provideTarget)\n      return\n    case step['target:yes_today:no']:\n      await ctx.reply(provideToday)\n      return\n    case step['target:yes_today:yes']:\n      await ctx.reply(\n        `You already provided today's wake up time: ${extractTimestamp(\n          // eslint-disable-next-line @typescript-eslint/no-non-null-assertion\n          ctx.session.today!\n        )}`\n      )\n      return\n  }\n}\n\nfunction settings(ctx: Context) {\n  return JSON.stringify(ctx.session)\n}\n\nasync function updateDays(ctx: Context) {\n  // eslint-disable-next-line @typescript-eslint/no-non-null-assertion\n  const success = hasSucceed(ctx.session.target!, ctx.session.today!)\n  // eslint-disable-next-line @typescript-eslint/no-non-null-assertion\n  const days = ctx.session.days!\n\n  if (success) {\n    const res = days + 1\n    ctx.session.days = res\n    await ctx.reply(`Congrats, you rock! You are doing great for last ${res}`)\n    return\n  } else {\n    ctx.session.days = 0\n    await ctx.reply(\n      `Oops, the successful row of ${days} days has been broken. No worries, tomorrow is a new day and this time is going to be different.`\n    )\n    return\n  }\n}\n\nfunction hasSucceed(target: Date, actual: Date) {\n  if (actual.getHours() < target.getHours()) {\n    return true\n  }\n  if (\n    actual.getHours() == target.getHours() &&\n    actual.getMinutes() <= target.getMinutes()\n  ) {\n    return true\n  }\n}\n\nexport function extractTimestamp(date: Date) {\n  return `${date.getHours()}:${date.getMinutes().toString().padStart(2, '0')}`\n}\n\nexport function parseTimestamp(input: string | RegExpMatchArray | undefined) {\n  if (typeof input !== 'string') {\n    return null\n  }\n\n  const data = input.replace(':', '')\n  let hours, minutes\n\n  if (data.length == 4) {\n    // 1000\n    hours = data.slice(0, 2)\n    minutes = data.slice(2)\n  } else if (data.length == 3) {\n    // 900\n    hours = data.slice(0, 1)\n    minutes = data.slice(1)\n  } else {\n    return null\n  }\n\n  const date = new Date()\n\n  try {\n    date.setHours(parseInt(hours), parseInt(minutes))\n  } catch (e) {\n    console.info('failed to parse timestamp ', e)\n    return null\n  }\n\n  return date\n}\n
